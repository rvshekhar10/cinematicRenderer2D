{
  "schemaVersion": "1.0.0",
  "engine": {
    "targetFps": 60,
    "quality": "high"
  },
  "events": [{
    "id": "animations",
    "name": "Enhanced Animation Features",
    "scenes": ["loop", "yoyo", "keyframes", "stagger"]
  }],
  "scenes": [
    {
      "id": "loop",
      "name": "Looping Animation",
      "duration": 8000,
      "layers": [
        {
          "id": "bg",
          "type": "gradient",
          "zIndex": 1,
          "config": {
            "colors": ["#667eea", "#764ba2"],
            "direction": "to bottom"
          }
        },
        {
          "id": "circle",
          "type": "textBlock",
          "zIndex": 2,
          "config": {
            "text": "●",
            "fontSize": "80px",
            "color": "#ffffff",
            "textAlign": "center",
            "position": { "x": "20%", "y": "50%" },
            "transform": { "translateY": "-50%" }
          },
          "animations": [
            {
              "property": "config.position.x",
              "from": "20%",
              "to": "80%",
              "startMs": 0,
              "endMs": 2000,
              "easing": "ease-in-out",
              "loop": true
            }
          ]
        },
        {
          "id": "title",
          "type": "textBlock",
          "zIndex": 3,
          "config": {
            "text": "Loop Animation\n(Continuous Repeat)",
            "fontSize": "36px",
            "fontWeight": "bold",
            "color": "#ffffff",
            "textAlign": "center",
            "position": { "x": "50%", "y": "20%" },
            "transform": { "translateX": "-50%" },
            "lineHeight": 1.4
          }
        }
      ]
    },
    {
