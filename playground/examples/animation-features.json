            "text": "■",
            "fontSize": "80px",
            "color": "#ffffff",
            "textAlign": "center",
            "position": { "x": "50%", "y": "50%" },
            "transform": { "translateX": "-50%", "translateY": "-50%", "scale": 1 }
          },
          "animations": [
            {
              "property": "config.transform.scale",
              "from": 0.5,
              "to": 1.5,
              "startMs": 0,
              "endMs": 2000,
              "easing": "ease-in-out",
